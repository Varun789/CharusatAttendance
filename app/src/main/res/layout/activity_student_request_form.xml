<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".student_request_form"
    android:background="#00cdac">


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        android:padding="20dp">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:clickable="true"
            android:focusable="true"
            android:foreground="?android:attr/selectableItemBackground"
            android:padding="15dp">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-medium"
                android:text="Request Form"
                android:textColor="#FFFFFF"
                android:textSize="30dp"
                android:textStyle="bold" />
        </LinearLayout>
    </LinearLayout>

    <com.github.captain_miao.optroundcardview.OptRoundCardView
        android:id="@+id/optRoundCardView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:optRoundCardBackgroundColor="#ffffff"
        app:optRoundCardCornerRadius="60dp"
        app:optRoundCardLeftBottomCorner="false"
        app:optRoundCardLeftTopCorner="true"
        app:optRoundCardRightBottomCorner="false"
        app:optRoundCardRightTopCorner="true">
        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true"
            android:layout_marginTop="20dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="40dp"
                android:orientation="vertical">

                <androidx.cardview.widget.CardView
                    android:id="@+id/firstcard"
                    android:layout_width="match_parent"
                    android:layout_height="133dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginHorizontal="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd"
                    android:elevation="10dp"
                    android:padding="10dp">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginHorizontal="20dp"
                        android:layout_gravity="center_vertical"
                        >

                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Academic Year :"
                            android:layout_marginHorizontal="10dp"
                            android:textSize="14sp"
                            android:layout_marginTop="10sp"
                            android:textColor="#FFFFFF"
                            />
                        <Spinner
                            android:id="@+id/year"
                            android:layout_width="fill_parent"
                            android:layout_marginHorizontal="10dp"
                            android:layout_marginTop="20dp"
                            android:layout_height="wrap_content" />
                    </LinearLayout>

                </androidx.cardview.widget.CardView>




                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="133dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd"
                    android:outlineAmbientShadowColor="#d3d3d3">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginHorizontal="20dp"
                        android:layout_gravity="center_vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Sem:"
                            android:layout_marginHorizontal="10dp"
                            android:textSize="14sp"
                            android:layout_marginTop="10sp"
                            android:textColor="#FFFFFF"
                            />
                        <Spinner
                            android:id="@+id/sem"
                            android:layout_width="fill_parent"
                            android:layout_marginHorizontal="10dp"
                            android:layout_marginTop="20sp"
                            android:layout_height="wrap_content" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="133dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginHorizontal="20dp"
                        android:layout_gravity="center_vertical">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text=" Start Date :"
                                android:layout_marginHorizontal="10dp"
                                android:textSize="14sp"
                                android:layout_marginTop="10sp"
                                android:textColor="#FFFFFF"
                                />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <EditText
                                android:id="@+id/start_date_calender"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="DD/MM/YYYY "
                                android:textColor="#FFFFFF"
                                android:drawableRight="@drawable/ic_arrow_drop_down_black_24dp"
                                android:layout_marginHorizontal="10dp"
                                android:editable="false"
                                />


                        </LinearLayout>


                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="133dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginHorizontal="20dp"
                        android:layout_gravity="center_vertical">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="10dp"
                                android:layout_marginTop="10sp"
                                android:text="End Date :"
                                android:textColor="#FFFFFF"
                                android:textSize="14sp" />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <EditText
                                android:id="@+id/end_date_calender"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="DD/MM/YYYY "
                                android:textColor="#FFFFFF"
                                android:drawableRight="@drawable/ic_arrow_drop_down_black_24dp"
                                android:layout_marginHorizontal="10dp"
                                android:editable="false"
                                />
                        </LinearLayout>


                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="133dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginHorizontal="20dp"
                        android:layout_gravity="center_vertical">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text=" Start Time :"
                                android:layout_marginHorizontal="10dp"
                                android:textSize="14sp"
                                android:layout_marginTop="10sp"
                                android:textColor="#FFFFFF"
                                />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <EditText
                                android:id="@+id/edt_start_time"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="HH/MM"
                                android:onClick="setStartTime"
                                android:textColor="#FFFFFF"
                                android:drawableRight="@drawable/ic_arrow_drop_down_black_24dp"
                                android:layout_marginHorizontal="10dp"
                                android:editable="false"
                                />

                            <Spinner
                                android:id="@+id/am_pm_start_time"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20sp"
                                android:layout_marginRight="20sp" />


                        </LinearLayout>


                    </LinearLayout>

                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="133dp"
                    android:layout_marginHorizontal="30dp"
                    android:layout_marginTop="20dp"
                    app:cardBackgroundColor="#a1c4fd"
                    app:cardCornerRadius="12dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginHorizontal="20dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="10dp"
                                android:layout_marginTop="10sp"
                                android:text=" End Time :"
                                android:textColor="#FFFFFF"
                                android:textSize="14sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <EditText
                                android:id="@+id/edt_end_time"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="10dp"
                                android:drawableRight="@drawable/ic_arrow_drop_down_black_24dp"
                                android:editable="false"
                                android:onClick="setEndTime"
                                android:text="HH/MM"
                                android:textColor="#FFFFFF" />

                            <Spinner
                                android:id="@+id/am_pm_end_time"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20sp"
                                android:layout_marginRight="20sp" />


                        </LinearLayout>




                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="293dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="30dp"
                    android:layout_marginVertical="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginHorizontal="20dp"
                        android:layout_gravity="center_vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Participated In :"
                            android:layout_marginHorizontal="10dp"
                            android:textSize="14sp"
                            android:layout_marginTop="10sp"
                            android:textColor="#FFFFFF"
                            />

                        <RadioGroup
                            android:id="@+id/participation_in"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10sp">
                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Seminar"
                                android:layout_marginHorizontal="10dp"
                                android:textColor="#FFFFFF"/>

                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Workshop"
                                android:layout_marginHorizontal="10dp"
                                android:textColor="#FFFFFF"/>
                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Conference"
                                android:layout_marginHorizontal="10dp"
                                android:textColor="#FFFFFF"/>
                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Techfest"
                                android:layout_marginHorizontal="10dp"
                                android:textColor="#FFFFFF"/>
                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Sports"
                                android:layout_marginHorizontal="10dp"
                                android:textColor="#FFFFFF"/>
                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Other"
                                android:layout_marginHorizontal="10dp"
                                android:textColor="#FFFFFF"/>

                        </RadioGroup>
                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="283dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginHorizontal="20dp"
                        android:layout_gravity="center_vertical">


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Type of event"
                            android:layout_marginHorizontal="10dp"
                            android:textSize="14sp"
                            android:layout_marginTop="10sp"
                            android:textColor="#FFFFFF"
                            />

                        <RadioGroup
                            android:id="@+id/type_of_event"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10sp">

                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="10dp"
                                android:text="State"
                                android:textColor="#FFFFFF"/>

                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="10dp"
                                android:text="National"
                                android:textColor="#FFFFFF"/>

                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="10dp"
                                android:text="International"
                                android:textColor="#FFFFFF"/>

                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="10dp"
                                android:text="CHARUSAT"
                                android:textColor="#FFFFFF"/>

                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="10dp"
                                android:text="Cspit"
                                android:textColor="#FFFFFF"/>

                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="10dp"
                                android:text="Other"
                                android:textColor="#FFFFFF"/>

                        </RadioGroup>


                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="153dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd">

                    <LinearLayout
                        android:layout_width="282dp"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="20dp"
                        android:layout_gravity="center_vertical"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="10dp"
                            android:layout_marginTop="10sp"
                            android:text="Title of Event :"
                            android:textSize="14sp"
                            android:textColor="#FFFFFF"/>

                        <EditText
                            android:id="@+id/edt_event_title"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="18sp" />


                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="153dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd">

                    <LinearLayout
                        android:layout_width="287dp"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="20dp"
                        android:layout_gravity="center_vertical"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="10dp"
                            android:layout_marginTop="10sp"
                            android:text="Name of organization :"
                            android:textSize="14sp"
                            android:textColor="#FFFFFF"/>

                        <EditText
                            android:id="@+id/edt_name_of_organization"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="10dp"
                            android:textSize="18sp" />


                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="153dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginHorizontal="20dp"
                        android:layout_gravity="center_vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Topic of work :"
                            android:layout_marginHorizontal="10dp"
                            android:textSize="14sp"
                            android:layout_marginTop="10sp"
                            android:textColor="#FFFFFF"
                            />
                        <EditText
                            android:id="@+id/edt_topic"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="18sp"
                            android:layout_marginHorizontal="10dp"
                            />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="153dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginHorizontal="20dp"
                        android:layout_marginTop="20dp">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Have you participated single"
                            android:layout_marginHorizontal="10dp"
                            android:textSize="14sp"
                            android:layout_marginTop="10sp"
                            android:textColor="#FFFFFF"
                            />
                        <RadioGroup
                            android:id="@+id/group"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10sp">
                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Yes"
                                android:layout_marginHorizontal="10dp"
                                android:textColor="#FFFFFF"/>

                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="No"
                                android:layout_marginHorizontal="10dp"
                                android:textColor="#FFFFFF"/></RadioGroup>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="193dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginHorizontal="20dp"
                        android:layout_gravity="center_vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Have you received financial support from CHARUSAT/CSPIT"
                            android:layout_marginHorizontal="10dp"
                            android:textSize="14sp"
                            android:layout_marginTop="10sp"
                            android:textColor="#FFFFFF"
                            />
                        <RadioGroup
                            android:id="@+id/support"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10sp">
                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Yes"
                                android:layout_marginHorizontal="10dp"
                                android:textColor="#FFFFFF"/>

                            <RadioButton
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="No"
                                android:layout_marginHorizontal="10dp"
                                android:textColor="#FFFFFF"/></RadioGroup>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="153dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginHorizontal="30dp"
                    app:cardCornerRadius="12dp"
                    app:cardBackgroundColor="#a1c4fd">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginHorizontal="20dp"
                        android:layout_gravity="center_vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Any other information"
                            android:layout_marginHorizontal="10dp"
                            android:textSize="14sp"
                            android:layout_marginTop="10sp"
                            android:textColor="#FFFFFF"
                            />
                        <EditText
                            android:id="@+id/edt_info"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="18sp"
                            android:layout_marginHorizontal="10dp"
                            />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <Button
                        android:id="@+id/btn_cancel"
                        android:layout_width="170dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_marginLeft="20sp"
                        android:layout_marginBottom="30dp"
                        android:text="Cancel"
                        android:textAllCaps="false"
                        android:textColor="#FFFFFF"
                        android:background="@drawable/custom_btn"/>

                    <Button
                        android:id="@+id/btn_submit"
                        android:layout_width="170dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_marginLeft="30sp"
                        android:layout_marginBottom="30dp"
                        android:text="Submit"
                        android:textAllCaps="false"
                        android:textColor="#FFFFFF"
                        android:background="@drawable/custom_btn"
                       /></LinearLayout>

                <!--            </androidx.core.widget.NestedScrollView>-->
            </LinearLayout>


            <!--</ScrollView>-->
        </androidx.core.widget.NestedScrollView>



    </com.github.captain_miao.optroundcardview.OptRoundCardView>





</LinearLayout>